{
  "source": {
    "include": ["./"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
    "staticFiles": {
      "include": ["./screenshot-1.png", "./screenshot-2.png"]
    }

    }
  },
  "opts": {
    "recurse": true,
    "destination": "./docs/",
    "readme": "./Readme.md"
  }
}
